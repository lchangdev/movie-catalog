<!DOCTYPE html>

<html lang="en">
<head>
  <meta charset="utf-8">
  <title>Game Time</title>
  <link rel="stylesheet" href="styles.css">
</head>
<body>
<table>
  <thead>
    <tr>
      <th>Movie Title</th>
      <th>Year</th>
      <th>Rating</th>
      <th>Genre</th>
      <th>Studio Name</th>
    </tr>
  </thead>
  <% @movies_data.each do |movie| %>
    <tr>
      <td>
        <a href="#">
        <%= movie["title"] %>
        </a>
      </td>
      <td>
        <%= movie["year"] %>
      </td>
      <td>
        <%= movie["rating"] %>
      </td>
      <td>
        <%= movie["genre"] %>
      </td>
      <td>
        <%= movie["studio_name"] %>
      </td>
    </tr>
  <% end %>
</table>
</body>
</html>
